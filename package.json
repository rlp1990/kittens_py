{
  "name": "kittens-py",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --ci --coverage --reporters=default --reporters=jest-junit",
    "lint": "ng lint",
    "lint:fix": "eslint --ext .ts,.html,.scss --fix src",
    "prepare": "husky install",
    "pre-commit": "pretty-quick --staged && npm run lint:fix && npm test && npm run build",
    "pre-push": "npm run release"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.1.0",
    "@angular/common": "^18.1.0",
    "@angular/compiler": "^18.1.0",
    "@angular/core": "^18.1.0",
    "@angular/forms": "^18.1.0",
    "@angular/platform-browser": "^18.1.0",
    "@angular/platform-browser-dynamic": "^18.1.0",
    "@angular/router": "^18.1.0",
    "ng-zorro-antd": "^18.1.0",
    "rxjs": "~7.8.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.1.1",
    "@angular/cli": "^18.1.1",
    "@angular/compiler-cli": "^18.1.0",
    "@angular/language-server": "18.1.2",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "eslint-config-prettier": "9.1.0",
    "husky": "^9.1.4",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-preset-angular": "^14.2.0",
    "lint-staged": "15.2.7",
    "ng-mocks": "^14.13.0",
    "prettier": "^3.3.3",
    "pretty-quick": "4.0.0",
    "standard-version": "^9.5.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.3",
    "typescript": "~5.5.2"
  },
  "jest-junit": {
    "outputDirectory": "coverage",
    "outputName": "junit.xml",
    "classNameTemplate": "{classname}",
    "titleTemplate": "{title}"
  }
}
